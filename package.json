{
  "name": "cdk_extensions",
  "description": "One stop shop for most commonly used cdk (Cloud Development Kit) extensions",
  "version": "0.1.13",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@bazel/bazelisk": "1.7.3",
    "@bazel/buildifier": "3.5.0",
    "@bazel/ibazel": "latest",
    "@bazel/rollup": "^2.3.3",
    "@bazel/typescript": "^2.3.3",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/busboy": "^0.2.3",
    "@types/jest": "^26.0.21",
    "@types/mime": "^2.0.3",
    "@types/node": "14.14.9",
    "@types/sharp": "^0.26.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "commitlint": "^11.0.0",
    "esbuild": "^0.9.3",
    "eslint": "^7.22.0",
    "form-data": "^3.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.41.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "ts-jest": "^26.5.4",
    "tsconfig-paths": "^3.9.0"
  },
  "dependencies": {
    "@aws-cdk/assert": "^1.94.1",
    "@aws-cdk/aws-apigateway": "^1.94.1",
    "@aws-cdk/aws-certificatemanager": "^1.94.1",
    "@aws-cdk/aws-cloudfront": "^1.94.1",
    "@aws-cdk/aws-cloudfront-origins": "^1.94.1",
    "@aws-cdk/aws-iam": "^1.94.1",
    "@aws-cdk/aws-lambda-nodejs": "^1.94.1",
    "@aws-cdk/aws-route53": "^1.94.1",
    "@aws-cdk/aws-s3": "^1.94.1",
    "@aws-cdk/aws-sns": "^1.94.1",
    "@aws-cdk/aws-ssm": "^1.94.1",
    "@aws-cdk/core": "^1.94.1",
    "@slack/web-api": "^5.15.0",
    "aws-cdk": "^1.94.1",
    "aws-sdk": "^2.866.0",
    "busboy": "^0.3.1",
    "change-case": "^4.1.2",
    "mime": "^2.5.2",
    "sharp": "^0.26.3",
    "standard-version": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "bazel build ${TARGET:-//...}",
    "build:watch": "ibazel build ${TARGET:-//...}",
    "xtest": "bazel test ${TARGET:-//...}",
    "xtest:update": "bazel run ${TARGET}:test.update",
    "xtest:watch": "ibazel test ${TARGET:-//...}",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "lint": "eslint '**/*.ts' '**/*.js'",
    "bazel:format": "find . -type f \\( -name \"*.bzl\" -or -name WORKSPACE -or -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" | xargs buildifier -v --warnings=attr-cfg,attr-license,attr-non-empty,attr-output-default,attr-single-file,constant-glob,ctx-actions,ctx-args,depset-iteration,depset-union,dict-concatenation,duplicated-name,filetype,git-repository,http-archive,integer-division,load,load-on-top,native-build,native-package,out-of-order-load,output-group,package-name,package-on-top,positional-args,redefined-variable,repository-name,same-origin-load,string-iteration,unsorted-dict-items,unused-variable",
    "bazel:lint": "npm run bazel:format -- --lint=warn",
    "bazel:lint-fix": "npm run bazel:format -- --lint=fix",
    "format:check": "prettier --check ./**/*.ts",
    "format:write": "prettier --write ./**/*.ts",
    "pack:all": "./tools/release.sh pack",
    "publish:all": "./tools/release.sh publish",
    "clean": "bazel clean",
    "clean:deep": "bazel clean --expunge",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextFaze/cdk-extensions"
  },
  "homepage": "https://github.com/NextFaze/cdk-extensions/blob/master/README.md",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
