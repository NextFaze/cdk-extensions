{
  "name": "@cdkx/web-application",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Construct to setup hosting for static web application",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/NextFaze/cdk-extensions/packages/web-application"
  },
  "module": "ES6",
  "peerDependencies": {
    "@aws-cdk/core": "^1.64.0",
    "@aws-cdk/aws-cloudfront": "^1.64.0",
    "@aws-cdk/aws-route53": "^1.64.0",
    "@aws-cdk/aws-s3": "^1.64.0",
    "@aws-cdk/aws-certificatemanager": "^1.64.0",
    "@aws-cdk/aws-ssm": "^1.64.0",
    "@aws-cdk/aws-iam": "^1.64.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.64.0",
    "@aws-cdk/aws-apigateway": "^1.64.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.64.0"
  },
  "scripts": {
    "postinstall": "rm -rf node_modules/sharp & npm install --arch=x64 --platform=linux sharp"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "busboy": "^0.3.1",
    "sharp": "^0.26.2"
  },
  "keywords": [
    "cdk",
    "cdkx",
    "web",
    "spa",
    "hosting",
    "aws"
  ],
  "author": "Rushi Patel <rpatel@nextfaze.com>",
  "license": "MIT",
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false,
        "sharp": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
